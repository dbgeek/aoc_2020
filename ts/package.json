{
  "name": "aoc-2020",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "tsc",
    "test-ci": "jest",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watchAll"
  },
  "devDependencies": {
    "@types/jest": "^26.0.16",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.2.0",
    "jest": "^26.6.1",
    "prettier": "^2.1.1",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.3"
  },
  "dependencies": {},
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/d*.ts",
      "**/t*.ts",
      "!**/index.ts",
      "!**/*.d.ts",
      "!cdk.out/**/*",
      "!bin/**/*",
      "!webpack.config.ts"
    ]
  }
}
